<% flash.each do |type, msg| %>
  <div class= "alert">
    <%= msg %>
  </div>
<% end %>

<%= form_with model: @profile, local: true do |form| %> 
  <%= form.text_field :name, class: 'input' %> 
  <%= form.submit 'Criar', class: 'button is-link'%> 
<% end%>

<%= link_to 'Voltar', root_path, method: :get, class: 'button is-light' %>